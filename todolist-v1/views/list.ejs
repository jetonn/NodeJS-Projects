

<%- include("header") -%>

  <div class="box" id="heading">
    <h1><%= listTitle %></h1>
  </div>

  <div class="box">
      <%  for (var i=0; i<itemsArray.length; i++) { %>
        <div class="item">
          <input type="checkbox">
          <p><%= itemsArray[i] %></p>
        </div>
      <%  } %>

      <form class="item" action="/" method="POST">
        <input type="text" name="todoItem" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
      </form>
  </div>

<%- include("footer") -%>
